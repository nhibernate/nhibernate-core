<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
	assembly="NHibernate.Test"
	namespace="NHibernate.Test.Criteria.Lambda">

	<class name="Person">
		<id name="Id">
			<generator class="native"/>
		</id>
		<property name="Name" />
