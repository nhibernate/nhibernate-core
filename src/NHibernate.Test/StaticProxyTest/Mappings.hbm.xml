<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NHibernate.Test"
                   namespace="NHibernate.Test.StaticProxyTest">

  <class name="SimpleEntity">
    <id name="Id">
      <generator class="guid.comb"/>
    </id>
    <property name="Name"/>
    <property name="Text" column="`Text`"/>
  </class>

  <class name="InterfacedEntity" proxy="IEntity">
    <id name="Id">
      <generator class="guid.comb"/>
    </id>
    <property name="Name"/>
    <property name="Text" column="`Text`"/>
  </class>

  <class name="LazyTextEntity">
    <id name="Id">
      <generator class="guid.comb"/>
    </id>
    <property name="Name"/>
    <property name="Text" column="`Text`" lazy="true"/>
  </class>

  <class name="InterfacedLazyTextEntity" proxy="ILazyTextEntity">
    <id name="Id">
      <generator class="guid.comb"/>
    </id>
    <property name="Name"/>
    <property name="Text" column="`Text`" lazy="true"/>
  </class>

  <class name="OverridingEntity">
    <id name="Id">
      <generator class="guid.comb"/>
    </id>
    <property name="Name"/>
    <property name="Text" column="`Text`"/>
  </class>

  <!-- Names must be shorter than 30 characters for Firebird -->
  <class name="OverridingEntityChildWithOtherThanIdAccess" table="Oecwotia">
    <id name="Id">
      <generator class="guid.comb"/>
    </id>
    <property name="Name"/>
    <property name="Text" column="`Text`"/>
  </class>

  <class name="OverridingEntityWithField">
    <id name="Id">
      <generator class="guid.comb"/>
    </id>
    <property name="Name"/>
    <property name="Text" column="`Text`"/>
  </class>

  <class name="OverridingEntityWithFieldChild">
    <id name="Id">
      <generator class="guid.comb"/>
    </id>
    <property name="Name"/>
    <property name="Text" column="`Text`"/>
  </class>

</hibernate-mapping>
