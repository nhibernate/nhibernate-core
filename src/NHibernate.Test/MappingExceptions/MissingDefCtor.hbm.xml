<?xml version="1.0" encoding="utf-8" ?> 
<!--
	This is a valid mapping for a class that is missing
	a default no arg constructor.
-->
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0">
	<class name="NHibernate.Test.MappingExceptions.MissingDefCtor, NHibernate.Test">
		<id name="Id">
			<generator class="native" />
		</id>
		<property name="Something" />
	</class>

</hibernate-mapping>
