			<key-many-to-one name="Address" column="[AddressID]" access="field.camelcase-underscore"  />
			<key-many-to-one name="AddressType" column="[AddressTypeID]" access="field.camelcase-underscore"  />
			<key-many-to-one name="BusinessEntity" column="[BusinessEntityID]" access="field.camelcase-underscore"  />
		</composite-id>
		<property name="ModifiedDate" column="[ModifiedDate]" access="field.camelcase-underscore" not-null="true"/>
		<property name="Rowguid" column="[rowguid]" access="field.camelcase-underscore" not-null="true"/>
